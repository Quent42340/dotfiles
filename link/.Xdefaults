/* xconsole  */
xconsole*faceName:              Monospace:style=Regular:size=12
xconsole*background:            black
xconsole*foreground:            white
xconsole*geometry:              +0-0

/* xterm */
xterm*background:               black
xterm*foreground:               white
xterm*scrollBar:                false

xterm*VT100.geometry:     80x25
xterm*faceName:           Monospace:style=Regular:size=11
xterm*saveLines:          32767
xterm*scrollKey:          true
xterm*scrollTtyOutput:    false
xterm*rightScrollBar:     true
xterm*jumpScroll:         true
xterm*multiScroll:        true
xterm*toolBar:            false
xterm*borderWidth:		  0

! Tango color scheme
xterm*color0: #1e1e1e
xterm*color1: #cc0000
xterm*color2: #4e9a06
xterm*color3: #c4a000
xterm*color4: #3465a4
xterm*color5: #75507b
xterm*color6: #0b939b
xterm*color7: #d3d7cf
xterm*color8: #555753
xterm*color9: #ef2929
xterm*color10: #8ae234
xterm*color11: #fce94f
xterm*color12: #729fcf
xterm*color13: #ad7fa8
xterm*color14: #00f5e9
xterm*color15: #eeeeec

/* emacs */
emacs*ReverseVideo:		on

#include ".Xresources.d/rxvt-unicode"
#include ".Xresources.d/rxvt-unicode-colors"
#include ".Xresources.d/xft"

